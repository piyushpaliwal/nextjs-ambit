{
  "name": "nextjs-ambit",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "NODE_OPTIONS='--inspect' next dev",
    "lint": "next lint",
    "format": "prettier --write \"\\*_/_.+(js|json|tsx)\"",
    "build": "next build",
    "postbuild": "next-sitemap",
    "start": "next start",
    "sass": "sass --no-source-map src/assets/scss:public/css",
    "find:unused": "next-unused",
    "debug": "cross-env NODE_OPTIONS='--inspect' next dev"
  },
  "next-unused": {
    "alias": {},
    "include": [
      "src"
    ],
    "exclude": [],
    "entrypoints": [
      "pages"
    ]
  },
  "dependencies": {
    "@popperjs/core": "^2.11.5",
    "animate.css": "^4.1.1",
    "axios": "^1.2.1",
    "axios-hooks": "^4.0.0",
    "bootstrap": "^5.2.0",
    "clipboard": "^2.0.11",
    "dayjs": "^1.11.4",
    "glightbox": "^3.2.0",
    "gray-matter": "^4.0.3",
    "isotope-layout": "^3.0.6",
    "itooltip": "^1.1.1",
    "next": "^12.2.4-canary.9",
    "next-seo": "^6.0.0",
    "next-sitemap": "^3.1.44",
    "plyr-react": "^5.1.0",
    "progressbar.js": "^1.1.0",
    "react": "18.2.0",
    "react-countup": "^6.3.0",
    "react-dom": "18.2.0",
    "react-markdown": "^8.0.5",
    "react-scroll": "^1.8.7",
    "react-syntax-highlighter": "^15.5.0",
    "react-toc": "^3.1.0",
    "scrollcue": "^2.0.0",
    "sharp": "^0.31.1",
    "swiper": "^8.3.2",
    "typewriter-effect": "^2.19.0"
  },
  "devDependencies": {
    "@types/bootstrap": "^5.2.0",
    "@types/isotope-layout": "^3.0.9",
    "@types/node": "18.0.6",
    "@types/progressbar.js": "^1.1.2",
    "@types/react": "18.0.15",
    "@types/react-countup": "^4.3.1",
    "@types/react-dom": "18.0.6",
    "@types/react-scroll": "^1.8.4",
    "@types/react-syntax-highlighter": "^15.5.5",
    "cross-env": "^7.0.3",
    "eslint": "8.20.0",
    "eslint-config-next": "12.2.3",
    "eslint-config-prettier": "^8.5.0",
    "sass": "^1.54.0",
    "typescript": "4.7.4"
  }
}
